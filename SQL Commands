Create database quizbase;
Use quizbase;
Create table questions(
Qid int(260) auto_increment not null primary key,
Question varchar(200),
Ans_id int(260));

Create table answers (
Aid int(250) auto_increment not null primary key,
Anser varchar(250),	
Ans_id int(260));

Create table user(
Uid int(250) primary key auto_increment not null,
Username varchar(250),
Totalques int(250),
Answercorret int(250));

